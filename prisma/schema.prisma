generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Intervenant {
  id                         Int     @id @default(autoincrement())
  nom                        String  @map("Intervenants")
  genre                      String?
  filiere                    String? @map("Filière")
  promo                      String? @map("Promo")
  dansLePool                 Boolean? @map("Dans le Pool ?")
  email                      String? @map("Email")
  telephone                  String? @map("Téléphone")
  nbEtudesEnCours            Int?    @map("Nombre d'études en cours")
  nbEtudesTerminees          Int?    @map("Nombre d'études terminées")
  nbEtudesTotal              Int?    @map("Nombre d'études total")
  retributionTotale          Float?  @map("Rétribution totale")
  centreInterets             String? @map("Centre d'intérêts")
  competences                String? @map("Compétences")
}
